    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.4",
    "@types/uuid": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.1",
    "tsc-watch": "^6.0.4",
    "tslib": "1.9.3",
    "typescript": "^4.1.3"
